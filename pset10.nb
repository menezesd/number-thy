Cell[BoxData[
 RowBox[{
  RowBox[{"solveChineseRemainder", "[", "lst_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"aa", ",", "bb", ",", "mm", ",", " ", 
      RowBox[{"modulus", " ", "=", " ", "1"}], ",", " ", 
      RowBox[{"sol", " ", "=", " ", "0"}], ",", " ", "foo", ",", " ", "bar"}],
      "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"aa", " ", "=", " ", 
         RowBox[{
          RowBox[{"lst", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", "  ", 
        
        RowBox[{"bb", " ", "=", "  ", 
         RowBox[{
          RowBox[{"lst", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", "  ", 
        
        RowBox[{"mm", " ", "=", " ", 
         RowBox[{
          RowBox[{"lst", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "[", 
          RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", " ", 
        RowBox[{"foo", " ", "=", " ", 
         RowBox[{"aa", "*", "modulus"}]}], ";", "\[IndentingNewLine]", " ", 
        RowBox[{"bar", " ", "=", " ", 
         RowBox[{"solveLinearCongruence", "[", 
          RowBox[{"foo", ",", 
           RowBox[{"bb", " ", "-", " ", 
            RowBox[{"aa", "*", "sol"}]}], ",", " ", "mm"}], "]"}]}], ";", 
        "\[IndentingNewLine]", " ", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"bar", " ", "\[Equal]", " ", 
           RowBox[{"{", "}"}]}], ",", " ", 
          RowBox[{"Return", "[", 
           RowBox[{"{", "}"}], "]"}], ",", " ", 
          RowBox[{
           RowBox[{"sol", " ", "=", " ", 
            RowBox[{"sol", " ", "+", " ", 
             RowBox[{"modulus", "*", 
              RowBox[{"bar", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}]}]}], ";", " ", 
           RowBox[{"modulus", " ", "=", " ", 
            RowBox[{"modulus", " ", "*", " ", 
             RowBox[{"mm", "/", 
              RowBox[{"GCD", "[", 
               RowBox[{"aa", ",", "mm"}], "]"}]}]}]}], ";"}]}], "]"}]}], ",", 
       
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "lst", "]"}]}], "}"}]}], "]"}], ";", " ", 
     RowBox[{"{", 
      RowBox[{"sol", ",", "modulus"}], "}"}]}]}], "]"}]}]], "Input"],

